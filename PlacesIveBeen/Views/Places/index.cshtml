@using Places.Models;

<h1>Places I've Been</h1>

@if (Model.Count == 0)
{
  <p>There are no places in the list.</p>
}
<ul>
  @foreach (Places Places in Model)
  {
    <li>Place: @places.PlacesName<br> Travling Partner: @places.TravlingPartner<br> Journal: @places.JournalEntry<br> </li>
  }

</ul>
<a href="/Places/new">Add a new place.</a>
